{
  "env": {
    "node": true,
    "builtin": true
  },
  "globals": {
    "document": false,
    "event": false
  },
  "extends": "airbnb",
  "rules": {
    "block-scoped-var": 0,
    "camelcase": 0,
    "func-names": [1, "as-needed"],
    "no-var": 0,
    "comma-spacing": [ 1, {"before": false, "after": true}],
    "consistent-return": 2,
    "complexity": [ 1, 5 ],
    "curly": [ 2, "all"],
    "dot-notation": [ 1, { "allowKeywords": true }],
    "eqeqeq": [ 2, "smart"],
    "strict": [ 0, "never"],
    "guard-for-in": 2,
    "indent": [ 1, 2, {"SwitchCase": 1, "VariableDeclarator": 1}],
    "lines-around-comment": [ 1, {
      "beforeBlockComment": true,
      "beforeLineComment": true,
      "allowBlockStart": true,
      "allowObjectStart": true,
      "allowArrayStart": true
    }],
    "key-spacing": 0,
    "keyword-spacing": 1,
    "new-cap": 0,
    "no-alert": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [ 2, "except-parens"],
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-else-return": 1,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 0,
    "no-multi-str": 2,
    "no-multi-spaces": 1,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-param-reassign": 1,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-redeclare": 0,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": [ 2, { "allow": [ "i" ] }],
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unreachable": 1,
    "no-use-before-define": [ 1, { "functions": false } ],
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-with": 2,
    "no-unused-vars": [ 1, {"args": "none"}],
    "one-var": 0,
    "prefer-arrow-callback": 1,
    "quotes": [ 1, "double", "avoid-escape" ],
    "quote-props": [ 1, "consistent-as-needed"],
    "max-len": [ 1, 120, 2, {ignoreComments: true}],
    "semi": [ 0, "never"],
    "space-before-blocks": 1,
    "space-before-function-paren": [ 1, {
      "anonymous": "always",
      "named": "never"
    }],
    "space-in-parens": [ 2, "always"],
    "space-infix-ops": 1,
    "template-curly-spacing": [ 2, "always" ],
    "array-bracket-spacing": [ 2, "always" ],
    "object-curly-spacing": [ 2, "always" ],
    "computed-property-spacing": [ 2, "always" ],
    "no-multiple-empty-lines": [ 2, { "max": 1, "maxEOF": 0, "maxBOF": 0 } ],
    "valid-typeof": 2,
    "vars-on-top": 0,
    "wrap-iife": [ 2, "inside"]
  }
}
